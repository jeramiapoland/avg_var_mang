@article{levy_experimental_2002,
	title = {Experimental test of the prospect theory value function: {A} stochastic dominance approach},
	volume = {89},
	issn = {0749-5978},
	shorttitle = {Experimental test of the prospect theory value function},
	url = {http://www.sciencedirect.com/science/article/pii/S0749597802000110},
	doi = {10.1016/S0749-5978(02)00011-0},
	abstract = {According to prospect theory, individuals are risk averse regarding gains but risk seeking regarding losses, implying an S-shaped value function. The S-shaped value function hypothesis is based on experiments in which subjects are asked to choose separately between alternatives with either only positive or only negative outcomes, alternatives which rarely exist in the capital market. In addition, the S-shaped findings may be biased by the “certainty effect” and by probability distortion. In this paper we employ the recently developed prospect stochastic dominance criterion to test the prospect theory S-shaped value function hypothesis with mixed outcomes and with no “certainty effect.” Assuming that subjects do not distort moderate probabilities, we strongly reject the prospect theory S-shaped value function, with at least 76–86\% of the choices being inconsistent with such preferences. When possible subjective probability distortions are taken into account, we find that at least 50–66\% of the choices are inconsistent with an S-shaped value function.},
	number = {2},
	urldate = {2018-08-17},
	journal = {Organizational Behavior and Human Decision Processes},
	author = {Levy, Haim and Levy, Moshe},
	month = nov,
	year = {2002},
	pages = {1058--1081},
	file = {ScienceDirect Full Text PDF:/home/jeramia/.zotero/zotero/fb5ioud9.default/zotero/storage/APL228LH/Levy and Levy - 2002 - Experimental test of the prospect theory value fun.pdf:application/pdf;ScienceDirect Snapshot:/home/jeramia/.zotero/zotero/fb5ioud9.default/zotero/storage/UC8A386X/S0749597802000110.html:text/html}
