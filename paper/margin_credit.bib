volume = {23},
year = {2010}
}
@incollection{Rapach2013,
abstract = {We survey the literature on stock return forecasting, highlighting the challenges faced by forecasters as well as strategies for improving return forecasts. We focus on U.S. equity premium forecastability and illustrate key issues via an empirical application based on updated data. Some studies argue that, despite extensive in-sample evidence of equity premium predictability, popular predictors from the literature fail to outperform the simple historical average benchmark forecast in out-of-sample tests. Recent studies, however, provide improved forecasting strategies that deliver statistically and economically significant out-of-sample gains relative to the historical average benchmark. These strategies - including economically motivated model restrictions, forecast combination, diffusion indices, and regime shifts - improve forecasting performance by addressing the substantial model uncertainty and parameter instability surrounding the data-generating process for stock returns. In addition to the U.S. equity premium, we succinctly survey out-of-sample evidence supporting U.S. cross-sectional and international stock return forecastability. The significant evidence of stock return forecastability worldwide has important implications for the development of both asset pricing models and investment management strategies. {\textcopyright} 2013 Elsevier B.V.},
author = {Rapach, David and Zhou, Guofu},
booktitle = {Handbook of Economic Forecasting},
doi = {10.1016/B978-0-444-53683-9.00006-2},
editor = {Elliott, Graham and Timmermann, Allan},
file = {:C$\backslash$:/Users/20084/Dropbox/Gyan/Tail{\_}Risk/Tail Risk Project/Literature/methodology references/Rapach-Zhou-2013 Forecasting Stock Returns.pdf:pdf},
isbn = {9780444536839},
issn = {15740706},
keywords = {Asset allocation,Asset pricing model,Business cycle,Diffusion index,Economic variables,Equity premium,Forecast combination,Regime shifts,Technical indicators},
mendeley-groups = {Margin{\_}Credit},
pages = {327--383},
publisher = {Elsevier B.V.},
title = {{Forecasting stock returns}},
url = {http://dx.doi.org/10.1016/B978-0-444-53683-9.00006-2},
volume = {2A},
year = {2013}
}
@article{Rappoport1994,
abstract = {This article looks at the factors that resulted to the crash of the stock market in 1929 and 1987. Most of the 42-percent gain of the market in 1987 was erased by a two-day decline of 23 percent in October. Similarly, as measured by the Dow Jones Industrial Average, the market had gained almost 50 percent in 1928 and 25 percent in 1929. Then, on October 28 and 29, 1929, the index fell 23 percent. Some observers have argued that these were the outcome of the collapse of self-fulfilling rational bubbles because there was no apparent large shift in any fundamental factor. Examining the S{\&}P 500 futures options for evidence of expectations of a crash, researcher David Bates suggested that the 1987 crash had, ex post, the appearance of a rational bubble, with the dramatic gains of one year suddenly erased. In a model for pricing these options with a jump-diffusion process, he found that the parameters implied by market prices indicated that a crash was expected and that implicit distributions were negatively skewed. In this paper the authors find similar evidence of a fear of a crash in 1929 well in advance of the event. While there were no well-organized options markets in the 1920s, there was a very active market in brokers' loans, which were collateralized by stock. Using monthly data on time loans, implied volatility was measured by pricing these loans as options. Section I discusses the stylized facts about the 1929 bull market and the characteristics of brokers' loans. Section II develops the appropriate options-pricing model for brokers' loans, and Section III examines the implied volatilities produced by the data. Conclusions are presented in Section IV.},
author = {Rappoport, Peter and White, Eugene N},
file = {:C$\backslash$:/Users/20084/Dropbox/Gyan/Tail{\_}Risk/Tail Risk Project/Literature/content references/Rappaport{\_}White{\_}AER1994{\_}Margin{\_}Debt{\_}29Crash.pdf:pdf},
issn = {0002-8282},
journal = {American Economic Review},
keywords = {1929,1987,SECURITIES markets,STOCK Market Crash,VOLATILITY (Finance)},
mendeley-groups = {Margin{\_}Credit},
number = {1},
pages = {271--281},
title = {{Was the crash of 1929 expected?}},
url = {http://search.ebscohost.com/login.aspx?direct=true{\&}db=bth{\&}AN=9406080305{\&}site=ehost-live{\&}scope=site},
volume = {84},
year = {1994}
}

